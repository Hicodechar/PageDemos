 			return -EINVAL;
 
 		len = min(vma-&gt;vm_end, end) - addr;
<span class="p_del">-		zap_page_range(vma, addr, len, NULL);</span>
<span class="p_add">+		zap_page_range(vma, addr, len);</span>
 		trace_mpx_unmap_zap(addr, addr+len);
 
 		vma = vma-&gt;vm_next;
