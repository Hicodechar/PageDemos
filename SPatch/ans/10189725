<span class="p_del">-#ifndef __ISP</span>
<span class="p_del">-#if defined(MODE)</span>
<span class="p_del">-#define MODE aap</span>
<span class="p_del">-#error &quot;isp_exprs.h: is mode independent, but MODE is set&quot;</span>
<span class="p_del">-#endif</span>
<span class="p_del">-#if defined(VARIABLE_RESOLUTION)</span>
<span class="p_del">-#define VARIABLE_RESOLUTION noot</span>
<span class="p_del">-#error &quot;isp_exprs.h: is mode independent, but VARIABLE_RESOLUTION is set&quot;</span>
<span class="p_del">-#endif</span>
<span class="p_del">-#if defined(DECI_FACTOR_LOG2)</span>
<span class="p_del">-#define DECI_FACTOR_LOG2 mies</span>
