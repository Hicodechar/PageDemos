<h2>Patch</h2>
<div class="patch">
<pre class="content">
<span class="p_header">diff --git a/sound/pci/trident/trident_main.c b/sound/pci/trident/trident_main.c</span>
<span class="p_header">index 64d3b8eba4bb..0eca3c370990 100644</span>
<span class="p_header">--- a/sound/pci/trident/trident_main.c</span>
<span class="p_header">+++ b/sound/pci/trident/trident_main.c</span>
<span class="p_chunk">@@ -3363,11 +3363,9 @@</span> <span class="p_context"> static int snd_trident_tlb_alloc(struct snd_trident *trident)</span>
 	trident-&gt;tlb.entries_dmaaddr = ALIGN(trident-&gt;tlb.buffer.addr, SNDRV_TRIDENT_MAX_PAGES * 4);
 	/* allocate shadow TLB page table (virtual addresses) */
 	trident-&gt;tlb.shadow_entries = vmalloc(SNDRV_TRIDENT_MAX_PAGES*sizeof(unsigned long));
<span class="p_del">-	if (trident-&gt;tlb.shadow_entries == NULL) {</span>
<span class="p_del">-		dev_err(trident-&gt;card-&gt;dev,</span>
<span class="p_del">-			&quot;unable to allocate shadow TLB entries\n&quot;);</span>
<span class="p_add">+	if (!trident-&gt;tlb.shadow_entries)</span>
 		return -ENOMEM;
<span class="p_del">-	}</span>
<span class="p_add">+</span>
 	/* allocate and setup silent page and initialise TLB entries */
 	if (snd_dma_alloc_pages(SNDRV_DMA_TYPE_DEV, snd_dma_pci_data(trident-&gt;pci),
 				SNDRV_TRIDENT_PAGE_SIZE, &amp;trident-&gt;tlb.silent_page) &lt; 0) {

</pre>
