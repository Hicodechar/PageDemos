<span class="quote">&gt; -		printk(KERN_ERR &quot;Fence signaled invalid fence object handle &quot;</span>
<span class="quote">&gt; -		       &quot;0x%08lx.\n&quot;,</span>
<span class="quote">&gt; +		pr_err(&quot;Fence signaled invalid fence object handle 0x%08lx\n&quot;,</span>
<span class="quote">&gt;  		       (unsigned long)arg-&gt;handle);</span>
<span class="quote">&gt;  		return -EINVAL;</span>
<span class="quote">&gt;  	}</span>
