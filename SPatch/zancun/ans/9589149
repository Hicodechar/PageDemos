<span class="p_add">+static struct perf_evsel *perf_stat__find_event(struct perf_evlist *evsel_list,</span>
<span class="p_add">+						const char *name)</span>
<span class="p_add">+{</span>
<span class="p_add">+	struct perf_evsel *c2;</span>
