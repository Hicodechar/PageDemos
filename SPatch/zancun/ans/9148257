<span class="p_add">+static int hpfs_show_options(struct seq_file *seq, struct dentry *root)</span>
<span class="p_add">+{</span>
<span class="p_add">+	struct hpfs_sb_info *sbi = hpfs_sb(root-&gt;d_sb);</span>
