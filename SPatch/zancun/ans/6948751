<span class="p_add">+/* A wrapper for the CONFIG_SHMEM dependent counter */</span>
<span class="p_add">+static inline unsigned long get_mm_counter_shmem(struct mm_struct *mm)</span>
<span class="p_add">+{</span>
<span class="p_add">+#ifdef CONFIG_SHMEM</span>
<span class="p_add">+	return get_mm_counter(mm, MM_SHMEMPAGES);</span>
